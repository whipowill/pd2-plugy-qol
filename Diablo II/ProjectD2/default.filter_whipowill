//=================================================
// QUEST
//=================================================

// Act I
// - Scroll of Inifuss (bks bkd)
// - Wirt's Leg (leg)
// - Horadric Malus (hdm)
ItemDisplay[bks OR bkd OR leg OR hdm]: %LIGHT_GRAY%A1 %GOLD%%NAME%%MAP-53%

// Act II
// - Book of Skill (ass)
// - Horadric Scroll (tr1)
// - Horadric Cube (box)
// - Staff of Kings (msf)
// - Viper Amulet (vip)
// - Horadric Staff (hst)
ItemDisplay[ass OR tr1 OR box OR msf OR vip OR hst]: %LIGHT_GRAY%A2 %GOLD%%NAME%%MAP-53%

// Act III
// - Jade Figurine (j34)
// - Golden Bird (g34)
// - Potion of Life (xyz)
// - The Gidbinn (g33)
// - Lam Esen's Tome (bbb)
// - Khalim's Eye (qey)
// - Khalim's Brain (qbr)
// - Khalim's Heart (qhr)
// - Khalim's Flail (qf1)
// - Khalim's Will (qf2)
// - Mephisto's Soulstone (mss)
// - Diablo's Soulstone (cx6)
ItemDisplay[j34 OR g34 OR xyz OR g33 OR bbb OR qey OR qbr OR qhr OR qf1 OR qf2 OR mss OR cx6]: %LIGHT_GRAY%A3 %GOLD%%NAME%%MAP-53%

// Act IV
// - Hellforge Hammer (hfh)
ItemDisplay[hfh]: %LIGHT_GRAY%A4 %GOLD%%NAME%%MAP-53%

// Act V
// - Malah's Potion (???)
// - Scroll of Resistance (tr2)
ItemDisplay[tr2]: %LIGHT_GRAY%A5 %GOLD%%NAME%%MAP-53%

// Pandemonium
// - Diablo's Horn (dhn)
// - Baal's Eye (bey)
// - Mephisto's Brain (mbr)
// - Standard of Heroes (std)
// - Keys (pk1, pk2, pk3)
ItemDisplay[dhn OR bey OR mbr OR std OR pk1 OR pk2 OR pk3]: %LIGHT_GRAY%x %PURPLE%%NAME%%MAP-53%

// Absolution
// - Token of Absolution (toa)
// - Twisted Essence of Suffering (tes)
// - Charged Essense of Hatred (ceh)
// - Burning Essence of Terror (bet)
// - Festering Essence of Destruction (fed)
ItemDisplay[toa OR tes OR ceh OR bet OR fed]: %LIGHT_GRAY%x %ORANGE%%NAME%%MAP-53%

//=================================================
// GEAR
//=================================================

// Unique Items
ItemDisplay[UNI (NORM OR EXC OR ELT)]: %GOLD%%NAME%%CONTINUE%

// Set Items
ItemDisplay[SET (NORM OR EXC OR ELT)]: %GREEN%%NAME%%CONTINUE%

// Rare Items
ItemDisplay[RARE (NORM OR EXC OR ELT)]: %YELLOW%%NAME%%CONTINUE%

// Magic Items
ItemDisplay[MAG (NORM OR EXC OR ELT)]: %BLUE%%NAME%%CONTINUE%

// Crafted Items
ItemDisplay[!(NMAG OR MAG OR RARE OR SET OR UNI) (NORM OR EXC OR ELT)]: %ORANGE%%NAME%%CONTINUE%

// White Items
ItemDisplay[NMAG (NORM OR EXC OR ELT)]: %WHITE%%NAME%%CONTINUE%

// Icons
ItemDisplay[NORM]: %LIGHT_GRAY%0| %NAME%%CONTINUE%
ItemDisplay[EXC]: %LIGHT_GRAY%0|%RED%| %NAME%%CONTINUE%
ItemDisplay[ELT]: %LIGHT_GRAY%0|%RED%|%PURPLE%| %NAME%%CONTINUE%

// Ignore inferior items (except Wirt's leg)
ItemDisplay[FILTLVL>0 INF !leg]:

// Ignore 1os white items (except Wirt's leg)
ItemDisplay[FILTLVL>0 NMAG !leg !RW SOCK=1]:

// Ignore unusable white items (gloves, boots, belts)
ItemDisplay[FILTLVL>0 NMAG !(leg) (EQ4 OR EQ5 OR EQ6)]:

// Display ILVL on certain items
ItemDisplay[MAG (rin OR amu OR cm1 OR cm2 OR cm3)]: %NAME% %GRAY%%ILVL%%CONTINUE%

// Display Sockets
ItemDisplay[SOCK>0 !RW NMAG]: %NAME% %GRAY%(%SOCKETS%)%CONTINUE%

// Display Ethereal
ItemDisplay[ETH]: %NAME% %GRAY%Eth%CONTINUE%

// Fix Sale Prices
ItemDisplay[]: %NAME%%DARK_GREEN%%CONTINUE% // only works here bc I never add to end of string beyond this point

//=================================================
// QUIVERS
//=================================================

// Icons
ItemDisplay[MAG (aqv OR cqv)]: %LIGHT_GRAY%||| %BLUE%%NAME%%CONTINUE%
ItemDisplay[RARE (aqv OR cqv)]: %LIGHT_GRAY%||| %YELLOW%%NAME%%CONTINUE%

// Highlights
ItemDisplay[(MAG OR RARE) (aqv OR cqv) CLSK0>0]: %LIGHT_GRAY%+%NAME%%CONTINUE% // amazon skill
ItemDisplay[(MAG OR RARE) (aqv OR cqv) TABSK0>0]: %LIGHT_GRAY%+%NAME%%CONTINUE% // bow skill
ItemDisplay[(MAG OR RARE) (aqv OR cqv) STAT93>19]: %LIGHT_GRAY%+%NAME%%CONTINUE% // faster attack speed
ItemDisplay[(MAG OR RARE) (aqv OR cqv) STAT48>19]: %LIGHT_GRAY%+%NAME%%CONTINUE% // fire
ItemDisplay[(MAG OR RARE) (aqv OR cqv) STAT51>19]: %LIGHT_GRAY%+%NAME%%CONTINUE% // lightning
ItemDisplay[(MAG OR RARE) (aqv OR cqv) STAT54>19]: %LIGHT_GRAY%+%NAME%%CONTINUE% // cold
ItemDisplay[(MAG OR RARE) (aqv OR cqv) STAT22>19]: %LIGHT_GRAY%+%NAME%%CONTINUE% // maxdmg
ItemDisplay[(MAG OR RARE) (aqv OR cqv) STAT218>0]: %LIGHT_GRAY%+%NAME%%CONTINUE% // maxdmg by lvl

